# McCulloch-Pitts - Operaci贸n NOT

import numpy as np

# Entradas posibles (solo una variable)
entradas = np.array([[0], [1]])

# Peso negativo (para invertir el resultado)
pesos = np.array([-1])

# Umbral de activaci贸n
umbral = 0

# Salidas calculadas
salidas = []

for x in entradas:
    # Suma ponderada
    suma = np.dot(x, pesos)
    
    # Funci贸n de activaci贸n (step)
    salida = 1 if suma >= umbral else 0
    salidas.append(salida)

print("Entradas:\n", entradas)
print("Salidas NOT:", salidas)
